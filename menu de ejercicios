Internal class Program
    {
        Static void Main(string[] args)
        {
          Double Cwhats, Pxwats, costo;
            Console.WriteLine(“introduce el costo por whats  “);
            Cwhats = double.Parse(Console.ReadLine());
            Console.WriteLine(“introduce los whats consumidos”);
            Pxwats = double.Parse(Console.ReadLine());
            Costo =  (Cwhats * Pxwats);
            Console.WriteLine(“Consumo de energia”+ costo);
            Console.ReadKey();
        }
    }
using System;
using System.Linq;

namespace Precio_de_galon
{
	public static class Program
	{
		public static void Main()
		{
			 Double Internet, Horas, Total;
			Console.WriteLine("Ingresa el precio por hora de Internet");
			Internet= double.Parse(Console.ReadLine());
			Console.WriteLine("Ingresa la cantidad de horas consumidas");
			Horas= double.Parse(Console.ReadLine());
			Total=Internet* Horas;
			Console.WriteLine("el costo de Horas"+Total);
		}
	}

